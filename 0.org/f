FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      f;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }

    blades
    {
        type            fWallFunction;
        value           $internalField;
    }

    shaft
    {
        type            fWallFunction;
        value           $internalField;
    }

    walls
    {
        type            fWallFunction;
        value           $internalField;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }

    #include "include/AMI"

//    #include "include/frontBackTopBottomPatches"
}

// ************************************************************************* //
