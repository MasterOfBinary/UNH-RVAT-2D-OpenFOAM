FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-4; // sqrt(3/2)*U*I*l

boundaryField
{
    blades
    {
        type            fixedValue;
        value           uniform 0;
    }

    shaft
    {
        type            fixedValue;
        value           uniform 0;
    }

    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }

    walls
    {
        type            fixedValue;
        value           uniform 0;
    }

    #include "include/AMI"
}


// ************************************************************************* //
