FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1e-5;

boundaryField
{
    #include "include/fixedInlet"

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    blades
    {
        type            epsilonLowReWallFunction;
        value           $internalField;
    }

    shaft
    {
        type            epsilonLowReWallFunction;
        value           $internalField;
    }

    #include "include/AMI"

    walls
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }

}


// ************************************************************************* //
