FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $turbulentKE;

boundaryField
{
    #include "include/fixedInlet"

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    "unh_rvat.*"
    {
        type            fixedValue; // Was kqRWallFunction
        value           uniform 1e-12; // Was $internalField
    }

    #include "include/AMI"

    topAndBottom
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }

//    #include "include/frontBackTopBottomPatches"
}


