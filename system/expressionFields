    pressureGradient
    {
        type expressionField;
        functionObjectLibs
        (
            "libswakFunctionObjects.so"
        );
        outputControl outputTime;
        fieldName gradP;
        dimension [1 -2 -2 0 0 0 0];
        expression "grad(p)";
        autowrite true;        
    }

