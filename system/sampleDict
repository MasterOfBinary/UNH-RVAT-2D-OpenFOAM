FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

setFormat raw;

interpolationScheme cellPoint;

sets
(
    profile
    {
        type        uniform;
        axis        y;
        start       (1.0 -1.5 0.05);
        end         (1.0 1.5 0.05);
        nPoints     301;
    }

    bladePath
    {
        type        circle;
        origin      (-0.1 0 0.05);
        circleAxis  (0 0 1);
        startPoint  (-0.1 0.5 0.05);
        dTheta      1; // degrees
        axis        xyz;         
    }
);

fields
(
    UMean
    UPrime2Mean
    kMean
);

