// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

    fieldAverage1
    {
        type            fieldAverage;
        functionObjectLibs ("libfieldFunctionObjects.so");
        enabled         true;
        outputControl   outputTime;
        resetOnRestart  false;
        resetOnOutput   false;

        fields
        (
            U
            {
                mean        on;
                prime2Mean  on;
                base        time;
            }

            p
            {
                mean        on;
                prime2Mean  off;
                base        time;
            }
            
            nut
            {
                mean        on;
                prime2Mean  off;
                base        time;
            }
            
            k
            {
                mean        on;
                prime2Mean  off;
                base        time;
            }
        );
    }
